
{
    "builds": [{
        "src": "news_api/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "250mb", "runtime": "python3.12" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "news_api/wsgi.py"
        }
    ]
    
  "functions": {
    "api/**": {
      "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
    }
  }
}
